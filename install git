//After install git - connect git & Github
git config --global user.name "ggprapakar"
git config --global user.email "govindaprapakaran28@gmail.com"
git config --list

// install git
git init

// add all files to git
git add .

//commit fiel change
git commit -m "my update message"

// upload my updated files to git live
git push origin 

// upload my updated files to git to following origin
git remote add origin https://github.com/lazzieecoderr/github.git

git push -u origin master
 
//For pushing the changes

git add .

git commit -m "commit message"

git push origin